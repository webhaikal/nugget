import init.qcom.ferrari.rc

on post-fs-data
    chown system system /dev/sysmatdrv
    chmod 0660 /dev/sysmatdrv

on boot
    mkdir /persist/bluetooth 0770 bluetooth bluetooth

# Vibrator
    chmod 0660 /dev/tspdrv
    chown root shell /dev/tspdrv
    chown system system /sys/class/timed_output/vibrator/enable

    # Notification LED permissions
    chown system system /sys/class/leds/red/blink
    chown system system /sys/class/leds/red/risetime
    chown system system /sys/class/leds/red/falltime
    chown system system /sys/class/leds/green/blink
    chown system system /sys/class/leds/green/risetime
    chown system system /sys/class/leds/green/falltime
    chown system system /sys/class/leds/blue/blink
    chown system system /sys/class/leds/blue/risetime
    chown system system /sys/class/leds/blue/falltime

    # Reset LEDs brightness
    write /sys/class/leds/red/brightness 0
    write /sys/class/leds/green/brightness 0
    write /sys/class/leds/blue/brightness 0.

    # charger
    chown system system /sys/class/power_supply/battery/capacity
    chmod 0660 /sys/class/power_supply/battery/capacity
    chown root /sys/class/power_supply/battery/current_now
    chown root /sys/class/power_supply/battery/voltage_now
    chmod 0664 /sys/class/power_supply/battery/current_now
    chmod 0664 /sys/class/power_supply/battery/voltage_now

    ## cmhw permissions
    # key disabler
    chown system system /sys/devices/soc.0/78b9000.i2c/i2c-5/5-004a/disable_keys

    # doubletap to wake
    chown system system /sys/devices/soc.0/78b9000.i2c/i2c-5/5-004a/wakeup_mode
    chmod 0660 /sys/devices/soc.0/78b9000.i2c/i2c-5/5-004a/wakeup_mode

    # Configure UKSM
    chown root system /sys/kernel/mm/uksm/sleep_millisecs
    chmod 0664 /sys/kernel/mm/uksm/sleep_millisecs
    write /sys/kernel/mm/uksm/run 1
    write /sys/kernel/mm/uksm/sleep_millisecs 500

