    chown system system /sys/devices/soc.0/78b6000.i2c/i2c-0/0-0008/lsm6db0_accel* /iio:device* scan_elements/in_accel_y_en
    chown system system /sys/devices/soc.0/78b6000.i2c/i2c-0/0-0008/lsm6db0_accel* /iio:device* scan_elements/in_accel_z_en
